<%- include ('../../partials/header.ejs') %>
<%- include ('../../partials/navbar.ejs') %>

<div class="mt-3 container">
    <h2>Usuários</h2>
    <a href="/admin/users/create" class="btn-success btn">Novo Usuário</a>


    <table class="table mt-4 table-bordered">
        <thead>
            <tr>
                <th>ID</th>
                <th>Email</th>
            </tr>
        </thead>    
        <tbody class="">    
            <% users.forEach(user => { %>
                <tr>
                    <td><%= user.id%></td>
                    <td><%= user.email%></td>
                </tr>

           <% }) %>
        </tbody>    
    </table>

</div>

<%- include ('../../partials/footer.ejs') %>